{
  "version": 2,
  "builds": [
    {
      "src": "/index.php",
      "use": "vercel-php@0.7.4"
    },
    {
      "src": "/assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}